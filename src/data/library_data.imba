import {collections_data} from './collections_data.imba'

# NOTE: Converts old collection data to new flatter data structure for instantDB.
class NewLibraryFromCollections
	new_lib = {collections:{}, lessons:{}, phrases:{}, weight: 0, words:{}}
	test = 'test'
	def constructor collections
		for collection, ci in collections
			let col_key = "{(ci+1)}" # ex. "01"
			let new_col = {}
			new_col.lesson_keys = []
			let lessons = collection.lessons
			
			for lesson, li in lessons
			
				let lesson_key = "{(ci+1)}-{(li+1)}" # ex. "01-01"
				let new_lesson = {}
				new_col.lesson_keys.push(lesson_key) # ex. "01-02"
				new_lesson.phrase_keys = []
			
				for phrase, pi in lesson.phrases
					let phrase_key = "{(ci+1)}-{(li+1)}-{(pi+1)}" # ex. "01-02-13"
					let new_phrase = {}
					new_phrase.c = ci+1
					new_phrase.l = li+1
					new_phrase.p = pi+1
					new_phrase.isfirst = if (pi + 1) is 1 then true else false
					new_phrase.islast = if (pi + 1) is lesson.phrases.length then true else false
					# new_phrase.lesson_key = lesson_key # ex. "01-01" # DELETE
					# new_phrase.collection_key = col_key # ex. "01" # DELETE
					new_phrase.img = phrase.image # ex. "14-2"
					new_phrase.order = phrase.index # ex. 2
					new_phrase.type = "phrase"
					new_phrase.phrase = phrase.khmer.split('|') # ex. "ព្រះ|ទ្រង់|បាន|សន្យា"
					new_phrase.translation = phrase.meaning # ex. "2 God had promised Abraham"
					new_phrase.words = {}
					for own word, weight of phrase.word_usage_count
						new_phrase.words[word] = {weight: weight}
					new_phrase.weight = addWeights(new_phrase.words, 'phrase') # ex. 42 — sum of all word usage counts
					new_lesson.phrase_keys.push(phrase_key) # ex. "01-02-13"
					new_lib.phrases[phrase_key] = new_phrase
				new_lesson.isfirst = if (li + 1) is 1 then true else false
				new_lesson.islast = if (li + 1) is lessons.length then true else false
				new_lesson.type = "lesson"
				new_lesson.ci = ci+1
				new_lesson.li = li+1
				new_lesson.collection_key = (ci+1)
				new_lesson.img = lesson.image
				new_lesson.order = lesson.index # ex. 2
				new_lesson.title = lesson.title # ex. "Jesus goes to heaven"
				new_lesson.slug = lesson.route # ex. "jesus-ascension"
				new_lesson.words = {}
				for own word, weight of lesson.word_usage_count
					new_lesson.words[word] = {
						weight: (weight or 0)
					}
				new_lesson.weight = addWeights(new_lesson.words, 'lesson')
				new_lib.lessons[lesson_key] = new_lesson
				
			new_col.type = "collection"
			new_col.islast = if (ci + 1) is collections.length then true else false
			new_col.isfirst = if (ci + 1) is 1 then true else false
			new_col.ci = ci+1
			new_col.info = collection.info # ex. "Learn the vocabulary of ..."
			new_col.price = collection.price # ex. 0
			new_col.title = collection.title # ex. "Jesus goes to heaven"
			new_col.order = collection.route # ex. "jesus-ascension"
			new_col.icon = collection.icon # ex. "market"
			new_col.img = collection.image # ex. "14"
			new_col.title = collection.title # ex. "Jesus goes to heaven"
			new_col.slug = titleToSlug(new_col.title) # ex. "jesus-ascension"
			new_col.words = {}
			for own word, weight of collection.word_usage_count
				new_col.words[word] = {
					weight: (weight or 0)
				}
			new_col.weight = addWeights(new_col.words, 'col')
			new_lib.collections[col_key] = new_col
			WARN new_col.weight
			new_lib.weight += new_col.weight
			new_lib.words = joinWordWeights(new_lib.words, new_col.words)
		return new_lib
		
	def formatNum num
		num.toString().padStart(2, '0');
		
	def titleToSlug string
		let slug = string.toLowerCase()
		slug = slug.replace(/[^a-z0-9]+/g, "-")
		slug = slug.replace(/^-|-$/g, "")
		return slug
		
	def joinWordWeights obja, objb
		let word_weights = {}
		for own word, weight of obja
			word_weights[word] = weight
		for own word, weight of objb
			if word_weights[word]
				word_weights[word] += weight
			else
				word_weights[word] = weight
		return word_weights
	def calcWordWeights words_array, src
		let word_weights = {}
		for word in words_array
			if word_weights[word]
				word_weights[word] += 1
			else
				word_weights[word] = 1
		return word_weights

	def addWeights word_weights_obj, src
		let word_weights = 0
		for own word, value of word_weights_obj
			if value.weight isa 'number'
				word_weights += value.weight
			else
				WARN "error adding weights at {src} because value is not word.weight is not a number"
		return word_weights

