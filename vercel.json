{
    "version": 2,
    "builds": [
        { "src": "dist/**", "use": "@vercel/static" }
    ],
    "routes": [
        { 
            "src": "/[^.]+", 
            "dest": "/",
            "status": 200 
        },
        {
            "src": "/(.+)",
            "dest": "/dist/$1"
        }
    ]
}